                                                                                    <xs:documentation>{tpValor} = [17]**, {codIncCP} = [13, 14] e {grauExp} = 3;</xs:documentation>
                                                                                    <xs:documentation>{tpValor} = [18]**, {codIncCP} = [13, 14] e {grauExp} = 4;</xs:documentation>
                                                                                    <xs:documentation>{tpValor} = [19], {codIncCP} = [15, 16];</xs:documentation>
                                                                                    <xs:documentation>{tpValor} = [31], {codIncCP} = [51];</xs:documentation>
                                                                                    <xs:documentation>{tpValor} = [32], {codIncCP} = [21, 22];</xs:documentation>
                                                                                    <xs:documentation>{tpValor} = [41]*, {codIncCP} = [11, 12, 21, 22] e (({grauExp} = 1) ou (sem informação em {grauExp})), observado o limite para Categorias 107 e 108;</xs:documentation>
                                                                                    <xs:documentation>{tpValor} = [42]*, {codIncCP} = [11, 12, 21, 22] e {grauExp} = 2, observado o limite para Categorias 107 e 108;</xs:documentation>
                                                                                    <xs:documentation>{tpValor} = [43]*, {codIncCP} = [11, 12, 21, 22] e {grauExp} = 3, observado o limite para Categorias 107 e 108;</xs:documentation>

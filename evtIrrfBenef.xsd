                                                            <xs:documentation>Origem: campo {dtPgto} do S-1210.</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="codCateg" type="TS_codCateg">
                                                        <xs:annotation>

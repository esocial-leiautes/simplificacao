    <xs:include schemaLocation="include/ideEmpregador.xsd" />
    <xs:import namespace="http://www.w3.org/2000/09/xmldsig#" schemaLocation="xmldsig-core-schema.xsd" />
    <xs:element name="eSocial">
        <xs:annotation>
            <xs:documentation>S-5002 - Imposto de Renda Retido na Fonte por trabalhador</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="evtIrrfBenef">
                    <xs:annotation>
                        <xs:documentation>Evento IRRF</xs:documentation>
                        <xs:documentation>CHAVE_GRUPO: {Id}</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="ideEvento">
                                <xs:annotation>
                                    <xs:documentation>Identificação do evento de retorno</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="nrRecArqBase" type="TS_nrRecibo">
                                            <xs:annotation>
                                                <xs:documentation>Preencher com o número do recibo do arquivo que deu origem ao presente arquivo de retorno ao empregador.</xs:documentation>
                                                <xs:documentation>Validação: Deve corresponder ao recibo de um arquivo com informações de rendimentos sujeitos a IRRF (S-1210 ou S-3000).</xs:documentation>
                                            </xs:annotation>
                                        </xs:element>
                                        <xs:element name="perApur" type="TS_perApur_mensal">
                                            <xs:annotation>
                                                <xs:documentation>Informar o mês/ano (formato AAAA-MM) de referência das informações.</xs:documentation>
                                            </xs:annotation>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="ideEmpregador" type="T_ideEmpregador" />
                            <xs:element name="ideTrabalhador">
                                <xs:annotation>
                                    <xs:documentation>Identificação do beneficiário</xs:documentation>
                                    <xs:documentation>CHAVE_GRUPO: {cpfBenef}</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="cpfBenef" type="TS_cpf">
                                            <xs:annotation>
                                                <xs:documentation>Número de inscrição no Cadastro de Pessoa Física - CPF do beneficiário do pagamento.</xs:documentation>
                                                <xs:documentation>Origem: campo {cpfBenef} do S-1210.</xs:documentation>
                                            </xs:annotation>
                                        </xs:element>
                                        <xs:element name="dmDev" maxOccurs="unbounded">
                                            <xs:annotation>
                                                <xs:documentation>Informações do demonstrativo de valores devidos</xs:documentation>
                                                <xs:documentation>CHAVE_GRUPO: {perRef}, {ideDmDev}, {tpPgto}, {dtPgto}</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="perRef" type="TS_perApur">
                                                        <xs:annotation>
                                                            <xs:documentation>Período de referência das informações, no formato AAAA-MM (ou AAAA, se for relativa à folha de 13° salário).</xs:documentation>
                                                            <xs:documentation>Origem: campo {perRef} do S-1210.</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="ideDmDev" type="TS_codigo_esocial">
                                                        <xs:annotation>
                                                            <xs:documentation>Identificador atribuído pela fonte pagadora para o demonstrativo de valores devidos ao trabalhador.</xs:documentation>
                                                            <xs:documentation>Origem: campo {ideDmDev} do S-1210.</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="tpPgto">

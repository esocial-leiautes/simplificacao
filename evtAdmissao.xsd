<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns="http://www.esocial.gov.br/schema/evt/evtAdmissao/v01_00_00" targetNamespace="http://www.esocial.gov.br/schema/evt/evtAdmissao/v01_00_00" xmlns:ds="http://www.w3.org/2000/09/xmldsig#" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
    <xs:include schemaLocation="include/ideEvento_trab.xsd " />
    <xs:include schemaLocation="include/ideEmpregador.xsd" />
    <xs:include schemaLocation="include/nascimento.xsd" />
    <xs:include schemaLocation="include/endereco.xsd" />
    <xs:include schemaLocation="include/contato.xsd" />
    <xs:include schemaLocation="include/aprend.xsd" />
    <xs:include schemaLocation="include/horContratual.xsd" />
    <xs:include schemaLocation="include/alvaraJudicial.xsd" />
    <xs:include schemaLocation="include/treiCap.xsd" />
    <xs:include schemaLocation="include/remuneracao.xsd" />
    <xs:include schemaLocation="include/localTrabalho.xsd" />
    <xs:import namespace="http://www.w3.org/2000/09/xmldsig#" schemaLocation="xmldsig-core-schema.xsd" />
    <xs:element name="eSocial">
        <xs:annotation>
            <xs:documentation>S-2200 - Cadastramento Inicial do Vínculo e Admissão/Ingresso de Trabalhador</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="evtAdmissao">
                    <xs:annotation>
                        <xs:documentation>Evento Cadastramento Inicial do Vínculo e Admissão / Ingresso de Trabalhador</xs:documentation>
                        <xs:documentation>CHAVE_GRUPO: {Id}</xs:documentation>
                        <xs:documentation>REGRA:REGRA_ADMISSAO_POSTERIOR_INICIO_ATIVIDADES</xs:documentation>
                        <xs:documentation>REGRA:REGRA_ADMISSAO_VALIDA_DT_ADM</xs:documentation>
                        <xs:documentation>REGRA:REGRA_ADMISSAO_VALIDA_DURACAO_CONTRATO</xs:documentation>
                        <xs:documentation>REGRA:REGRA_BLOQUEIA_USO_CPF_EMPREGADOR</xs:documentation>
                        <xs:documentation>REGRA:REGRA_COMPATIBILIDADE_CATEGORIA_CLASSTRIB</xs:documentation>
                        <xs:documentation>REGRA:REGRA_COMPATIB_CATEG_EVENTO</xs:documentation>
                        <xs:documentation>REGRA:REGRA_ENVIO_PROC_FECHAMENTO</xs:documentation>
                        <xs:documentation>REGRA:REGRA_EVENTOS_EXTEMP</xs:documentation>
                        <xs:documentation>REGRA:REGRA_EVETRAB_VALIDA_OPCAO_FGTS</xs:documentation>
                        <xs:documentation>REGRA:REGRA_EXISTE_INFO_EMPREGADOR</xs:documentation>
                        <xs:documentation>REGRA:REGRA_EXTEMP_REINTEGRACAO</xs:documentation>
                        <xs:documentation>REGRA:REGRA_GERAL_VALIDA_DADOS_TABCONTRIB</xs:documentation>
                        <xs:documentation>REGRA:REGRA_MUDANCA_CPF</xs:documentation>
                        <xs:documentation>REGRA:REGRA_REGISTRO_PRELIMINAR</xs:documentation>
                        <xs:documentation>REGRA:REGRA_RETIFICA_MESMO_VINCULO</xs:documentation>
                        <xs:documentation>REGRA:REGRA_VALIDA_EMPREGADOR</xs:documentation>
                        <xs:documentation>REGRA:REGRA_VALIDA_MATRICULA</xs:documentation>
                        <xs:documentation>REGRA:REGRA_VALIDA_TRABALHADOR_BASE_CPF</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="ideEvento" type="T_ideEvento_trab" />
                            <xs:element name="ideEmpregador" type="T_ideEmpregador" />
                            <xs:element name="trabalhador">
                                <xs:annotation>
                                    <xs:documentation>Informações Pessoais do Trabalhador</xs:documentation>
                                    <xs:documentation>CHAVE_GRUPO: {cpfTrab}</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="cpfTrab" type="TS_cpf">
                                            <xs:annotation>
                                                <xs:documentation>CPF do trabalhador</xs:documentation>
                                            </xs:annotation>
                                        </xs:element>
                                        <xs:element name="nmTrab" type="TS_nome">
                                            <xs:annotation>
                                                <xs:documentation>Nome do Trabalhador</xs:documentation>
                                            </xs:annotation>
                                        </xs:element>
                                        <xs:element name="sexo" type="TS_sexo">
                                            <xs:annotation>
                                                <xs:documentation>Sexo</xs:documentation>
                                            </xs:annotation>
                                        </xs:element>
                                        <xs:element name="racaCor" type="TS_racaCor">
                                            <xs:annotation>
                                                <xs:documentation>Raça e cor do trabalhador</xs:documentation>
                                            </xs:annotation>
                                        </xs:element>
                                        <xs:element name="estCiv" minOccurs="0" type="TS_estCivil">
                                            <xs:annotation>
                                                <xs:documentation>Estado civil do trabalhador</xs:documentation>
                                            </xs:annotation>
                                        </xs:element>
                                        <xs:element name="grauInstr" type="TS_grauInstr">
                                            <xs:annotation>
                                                <xs:documentation>Grau de instrução do trabalhador</xs:documentation>
                                            </xs:annotation>
                                        </xs:element>
                                        <xs:element name="nmSoc" minOccurs="0" type="TS_nome">
                                            <xs:annotation>
                                                <xs:documentation>Nome Social</xs:documentation>
                                                <xs:documentation>Nome social para travesti ou transexual</xs:documentation>
                                            </xs:annotation>
                                        </xs:element>
                                        <xs:element name="nascimento" type="T_nascimento" />
                                        <xs:element name="endereco" minOccurs="0">
                                            <xs:annotation>
                                                <xs:documentation>Endereço do Trabalhador</xs:documentation>
                                                <xs:documentation>CONDICAO_GRUPO: O (se grupo {desligamento} não estiver preenchido); F (nos demais casos)</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:choice>
                                                        <xs:element name="brasil" type="T_endereco_Brasil">
                                                            <xs:annotation>
                                                                <xs:documentation>Endereço no Brasil</xs:documentation>
                                                                <xs:documentation>CONDICAO_GRUPO: O (se não informados os grupos {exterior} e {desligamento}); N (se grupo {exterior} estiver preenchido); F (nos demais casos)</xs:documentation>
                                                            </xs:annotation>
                                                        </xs:element>
                                                        <xs:element name="exterior" type="T_endereco_exterior">
                                                            <xs:annotation>
                                                                <xs:documentation>Endereço no Exterior</xs:documentation>
                                                                <xs:documentation>CONDICAO_GRUPO: O (se não informados os grupos {brasil} e {desligamento}); N (se grupo {brasil} estiver preenchido); F (nos demais casos)</xs:documentation>
                                                            </xs:annotation>
                                                        </xs:element>
                                                    </xs:choice>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="trabImig" minOccurs="0">
                                            <xs:annotation>
                                                <xs:documentation>Informações do trabalhador imigrante</xs:documentation>
                                                <xs:documentation>CONDICAO_GRUPO: N (se {paisNac} = Brasil); OC (se {paisNac} diferent de Brasil) e se grupo {desligamento} não estiver preenchido); F (nos demais casos)</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="tmpResid" minOccurs="0" type="TS_tmpResid">
                                                        <xs:annotation>
                                                            <xs:documentation>Tempo de residência do trabalhador imigrante</xs:documentation>
                                                            <xs:documentation>Validação: Preenchimento obrigatório se ({dtAdm} ou {dtExercicio}) >= [2020-09-08].</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="condIng" type="TS_condIng" />
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="infoDeficiencia" minOccurs="0">
                                            <xs:annotation>
                                                <xs:documentation>Pessoa com Deficiência</xs:documentation>
                                                <xs:documentation>CONDICAO_GRUPO: OC (se grupo {desligamento} não estiver preenchido); F (nos demais casos)</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="defFisica" type="TS_sim_nao">
                                                        <xs:annotation>
                                                            <xs:documentation>Deficiência Física</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="defVisual" type="TS_sim_nao">
                                                        <xs:annotation>
                                                            <xs:documentation>Deficiência visual</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="defAuditiva" type="TS_sim_nao">
                                                        <xs:annotation>
                                                            <xs:documentation>Deficiência auditiva</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="defMental" type="TS_sim_nao">
                                                        <xs:annotation>
                                                            <xs:documentation>Deficiência Mental</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="defIntelectual" type="TS_sim_nao">
                                                        <xs:annotation>
                                                            <xs:documentation>Deficiência Intelectual</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="reabReadap" type="TS_sim_nao">
                                                        <xs:annotation>
                                                            <xs:documentation>Reabilitado/Readaptado</xs:documentation>
                                                            <xs:documentation>Informar se o trabalhador é reabilitado (empregado) ou readaptado (servidor público/militar). Reabilitado: estando o empregado incapacitado parcial ou totalmente para o trabalho, cumpriu Programa de Reabilitação Profissional no INSS, recebendo certificado, sendo proporcionado os meios indicados para participar do mercado de trabalho. Readaptado: o servidor está investido em cargo de atribuições e responsabilidades compatíveis com a limitação que tenha sofrido em sua capacidade física ou mental verificada em inspeção médica.</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="infoCota" type="TS_sim_nao" minOccurs="0">
                                                        <xs:annotation>
                                                            <xs:documentation>Informar se o trabalhador preenche cota de pessoas com deficiência habilitadas ou de beneficiários reabilitados</xs:documentation>
                                                            <xs:documentation>Validação: Preenchimento obrigatório e exclusivo se {tpRegTrab} = [1].</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="observacao" minOccurs="0" type="TS_texto_255">
                                                        <xs:annotation>
                                                            <xs:documentation>Observação</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="dependente" minOccurs="0" maxOccurs="99">
                                            <xs:annotation>
                                                <xs:documentation>Informações dos dependentes</xs:documentation>
                                                <xs:documentation>CHAVE_GRUPO: {tpDep}, {nmDep}, {dtNascto}</xs:documentation>
                                                <xs:documentation>CONDICAO_GRUPO: OC</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="tpDep" type="TS_tpDep" />
                                                    <xs:element name="nmDep" type="TS_nome">
                                                        <xs:annotation>
                                                            <xs:documentation>Nome do Dependente</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="dtNascto">
                                                        <xs:simpleType>
                                                            <xs:annotation>
                                                                <xs:documentation>Data de Nascimento</xs:documentation>
                                                                <xs:documentation>Validação: Deve ser maior ou igual que 01/01/1890 e menor que a data atual.</xs:documentation>
                                                            </xs:annotation>
                                                            <xs:restriction base="xs:date">
                                                                <xs:minInclusive value="1890-01-01" />
                                                            </xs:restriction>
                                                        </xs:simpleType>
                                                    </xs:element>
                                                    <xs:element name="cpfDep" minOccurs="0" type="TS_cpf">
                                                        <xs:annotation>
                                                            <xs:documentation>Número de Inscrição no CPF</xs:documentation>
                                                            <xs:documentation>Validação: Deve ser um número de CPF válido, observando o que segue:</xs:documentation>
                                                            <xs:documentation>a) O preenchimento é obrigatório se {depIRRF} = [S];</xs:documentation>
                                                            <xs:documentation>b) Em arquivo de empregador Pessoa Física, deve ser diferente do CPF informado em {ideEmpregador};</xs:documentation>
                                                            <xs:documentation>c) Não pode haver mais de um dependente com o mesmo número de CPF.</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="sexoDep" minOccurs="0" type="TS_sexo">
                                                        <xs:annotation>
                                                            <xs:documentation>Sexo do dependente</xs:documentation>
                                                            <xs:documentation>Validação: Preenchimento obrigatório se {tpRegPrev} = [2] e {cadIni} = [N]. Não informar se {tpRegPrev} diferente de [2].</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="depIRRF" type="TS_sim_nao">
                                                        <xs:annotation>
                                                            <xs:documentation>Dependente para fins de IRRF</xs:documentation>
                                                            <xs:documentation>Informar se é dependente do trabalhador para fins de dedução de seu rendimento tributável pelo Imposto de Renda.</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="depSF" type="TS_sim_nao">
                                                        <xs:annotation>
                                                            <xs:documentation>Dependente SF</xs:documentation>
                                                            <xs:documentation>Informar se é dependente para fins de recebimento do benefício de salário-família.</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="incTrab" type="TS_sim_nao">
                                                        <xs:annotation>
                                                            <xs:documentation>Incapacidade para o trabalho</xs:documentation>
                                                            <xs:documentation>Informar se o dependente tem incapacidade física ou mental para o trabalho.</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="contato" type="T_contato" minOccurs="0">
                                            <xs:annotation>
                                                <xs:documentation>Informações de Contato</xs:documentation>
                                                <xs:documentation>CONDICAO_GRUPO: OC (se grupo {desligamento} não estiver preenchido); F (nos demais casos)</xs:documentation>
                                            </xs:annotation>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="vinculo">
                                <xs:annotation>
                                    <xs:documentation>Informações do Vínculo</xs:documentation>
                                    <xs:documentation>CHAVE_GRUPO: {matricula}</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="matricula" type="TS_codigo_esocial">
                                            <xs:annotation>
                                                <xs:documentation>Matricula</xs:documentation>
                                                <xs:documentation>Matrícula atribuída ao trabalhador pela empresa ou, no caso de servidor público, a matrícula constante no Sistema de Administração de Recursos Humanos do órgão.</xs:documentation>
                                                <xs:documentation>Validação: O valor informado não pode conter a expressão 'eSocial' nas 7 (sete) primeiras posições.</xs:documentation>
                                                <xs:documentation>REGRA:REGRA_CARACTER_ESPECIAL</xs:documentation>
                                            </xs:annotation>
                                        </xs:element>
                                        <xs:element name="tpRegTrab" type="TS_tpRegTrab">
                                            <xs:annotation>
                                                <xs:documentation>Regime trabalhista</xs:documentation>
                                                <xs:documentation>Validação: Se {codCateg} = [104], deve ser preenchido com [1].</xs:documentation>
                                            </xs:annotation>
                                        </xs:element>
                                        <xs:element name="tpRegPrev" type="TS_tpRegPrev">
                                            <xs:annotation>
                                                <xs:documentation>Regime previdenciário</xs:documentation>
                                                <xs:documentation>Validação: Se {codCateg} = [104], deve ser preenchido com [1]. Se {codCateg} = [101, 102, 103, 105, 106, 107, 108, 111], não pode ser preenchido com [2].</xs:documentation>
                                            </xs:annotation>
                                        </xs:element>
                                        <xs:element name="cadIni" type="TS_sim_nao">
                                            <xs:annotation>
                                                <xs:documentation>Indicativo de cadastramento inicial</xs:documentation>
                                                <xs:documentation>Indicar se o evento se refere a cadastramento inicial de vínculo (o ingresso do trabalhador no empregador declarante, por admissão ou transferência, é anterior à data de início da obrigatoriedade de envio de seus eventos não periódicos) ou se refere a uma admissão (o ingresso do trabalhador no empregador declarante é igual ou posterior à data de início de obrigatoriedade de envio de seus eventos não periódicos).</xs:documentation>
                                            </xs:annotation>
                                        </xs:element>
                                        <xs:element name="infoRegimeTrab">
                                            <xs:annotation>
                                                <xs:documentation>Informações do regime trabalhista</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:choice>
                                                        <xs:element name="infoCeletista">
                                                            <xs:annotation>
                                                                <xs:documentation>Informações de Trabalhador Celetista</xs:documentation>
                                                                <xs:documentation>CONDICAO_GRUPO: O (se {tpRegTrab} = [1]); N (nos demais casos)</xs:documentation>
                                                            </xs:annotation>
                                                            <xs:complexType>
                                                                <xs:sequence>
                                                                    <xs:element name="dtAdm" type="xs:date">
                                                                        <xs:annotation>
                                                                            <xs:documentation>Data de admissão do trabalhador</xs:documentation>
                                                                            <xs:documentation>No caso de transferência do empregado ou de mudança de CPF, preencher com a data inicial do vínculo no primeiro empregador (data de início do vínculo).</xs:documentation>
                                                                            <xs:documentation>Validação: Devem ser observadas as seguintes regras:</xs:documentation>
                                                                            <xs:documentation>a) Deve ser posterior à data de nascimento do trabalhador;</xs:documentation>
                                                                            <xs:documentation>b) Se {cadIni} = [S], deve ser anterior à data de início da obrigatoriedade dos eventos não periódicos para o empregador no eSocial;</xs:documentation>
                                                                            <xs:documentation>c) Se {cadIni} = [N] e {tpAdmissao} = [1], deve ser igual ou posterior à data de início da obrigatoriedade dos eventos não periódicos para o empregador no eSocial.</xs:documentation>
                                                                        </xs:annotation>
                                                                    </xs:element>
                                                                    <xs:element name="tpAdmissao">
                                                                        <xs:simpleType>
                                                                            <xs:annotation>
                                                                                <xs:documentation>Tipo de admissão do trabalhador</xs:documentation>
                                                                                <xs:documentation>Validação: Se for igual a [2,3,4], é obrigatório o preenchimento do registro de Sucessão de Vínculo Trabalhista. Se for igual a [5], {codCateg} deve ser igual a [104] e {procEmi} deve ser igual a [2]. Se for igual a [6], {cadIni} deve ser igual a [N].</xs:documentation>
                                                                            </xs:annotation>
                                                                            <xs:restriction base="xs:byte">
                                                                                <xs:enumeration value="1">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Admissão</xs:documentation>
                                                                                    </xs:annotation>
                                                                                </xs:enumeration>
                                                                                <xs:enumeration value="2">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Transferência de empresa do mesmo grupo econômico</xs:documentation>
                                                                                    </xs:annotation>
                                                                                </xs:enumeration>
                                                                                <xs:enumeration value="3">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Transferência de empresa consorciada ou de consórcio</xs:documentation>
                                                                                    </xs:annotation>
                                                                                </xs:enumeration>
                                                                                <xs:enumeration value="4">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Transferência por motivo de sucessão, incorporação, cisão ou fusão</xs:documentation>
                                                                                    </xs:annotation>
                                                                                </xs:enumeration>
                                                                                <xs:enumeration value="5">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Transferência do empregado doméstico para outro representante da mesma unidade familiar</xs:documentation>
                                                                                    </xs:annotation>
                                                                                </xs:enumeration>
                                                                                <xs:enumeration value="6">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Mudança de CPF</xs:documentation>
                                                                                    </xs:annotation>
                                                                                </xs:enumeration>
                                                                            </xs:restriction>
                                                                        </xs:simpleType>
                                                                    </xs:element>
                                                                    <xs:element name="indAdmissao">
                                                                        <xs:simpleType>
                                                                            <xs:annotation>
                                                                                <xs:documentation>Indicativo de Admissão</xs:documentation>
                                                                            </xs:annotation>
                                                                            <xs:restriction base="xs:byte">
                                                                                <xs:enumeration value="1">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Normal</xs:documentation>
                                                                                    </xs:annotation>
                                                                                </xs:enumeration>
                                                                                <xs:enumeration value="2">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Decorrente de Ação Fiscal</xs:documentation>
                                                                                    </xs:annotation>
                                                                                </xs:enumeration>
                                                                                <xs:enumeration value="3">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Decorrente de Decisão Judicial</xs:documentation>
                                                                                    </xs:annotation>
                                                                                </xs:enumeration>
                                                                            </xs:restriction>
                                                                        </xs:simpleType>
                                                                    </xs:element>
                                                                    <xs:element name="tpRegJor" type="TS_tpRegJor" />
                                                                    <xs:element name="natAtividade" type="TS_natAtividade">
                                                                        <xs:annotation>
                                                                            <xs:documentation>Natureza da atividade</xs:documentation>
                                                                            <xs:documentation>Validação: Se {codCateg} = [104], deve ser preenchido com [1]. Se {codCateg} = [102], deve ser preenchido com [2].</xs:documentation>
                                                                        </xs:annotation>
                                                                    </xs:element>
                                                                    <xs:element name="dtBase" minOccurs="0" type="TS_dtBase" />
                                                                    <xs:element name="cnpjSindCategProf" type="TS_cnpj">
                                                                        <xs:annotation>
                                                                            <xs:documentation>Preencher com o CNPJ do sindicato representativo da categoria (Preponderante ou Diferenciada)</xs:documentation>
                                                                            <xs:documentation>Validação: Deve ser um CNPJ válido, com 14 (catorze) algarismos.</xs:documentation>
                                                                        </xs:annotation>
                                                                    </xs:element>
                                                                    <xs:element name="FGTS" minOccurs="0">
                                                                        <xs:annotation>
                                                                            <xs:documentation>Informações do Fundo de Garantia do Tempo de Serviço - FGTS</xs:documentation>
                                                                            <xs:documentation>CONDICAO_GRUPO: N (se {tpAdmissao} = [6] OU (se {codCateg} diferente de [104] e {dtAdm} >= [1988-10-05]) OU (se {codCateg} = [104] e {dtAdm} >= [2015-10-01])); O (nos demais casos)</xs:documentation>
                                                                        </xs:annotation>
                                                                        <xs:complexType>
                                                                            <xs:annotation>
                                                                                <xs:documentation>Informações do FGTS</xs:documentation>
                                                                            </xs:annotation>
                                                                            <xs:sequence>
                                                                                <xs:element name="dtOpcFGTS" type="xs:date">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Data de Opção pelo FGTS</xs:documentation>
                                                                                    </xs:annotation>
                                                                                </xs:element>
                                                                            </xs:sequence>
                                                                        </xs:complexType>
                                                                    </xs:element>
                                                                    <xs:element name="trabTemporario" minOccurs="0">
                                                                        <xs:annotation>
                                                                            <xs:documentation>Dados sobre trabalho temporário</xs:documentation>
                                                                            <xs:documentation>CONDICAO_GRUPO: N (se {codCateg} diferente de [106]); O (se {codCateg} = [106] e se grupo {desligamento} não estiver preenchido); F (nos demais casos)</xs:documentation>
                                                                        </xs:annotation>
                                                                        <xs:complexType>
                                                                            <xs:sequence>
                                                                                <xs:element name="hipLeg">
                                                                                    <xs:simpleType>
                                                                                        <xs:annotation>
                                                                                            <xs:documentation>Hipótese legal para contratação de trabalhador temporário</xs:documentation>
                                                                                        </xs:annotation>
                                                                                        <xs:restriction base="xs:byte">
                                                                                            <xs:enumeration value="1">
                                                                                                <xs:annotation>
                                                                                                    <xs:documentation>Necessidade de substituição transitória de pessoal permanente</xs:documentation>
                                                                                                </xs:annotation>
                                                                                            </xs:enumeration>
                                                                                            <xs:enumeration value="2">
                                                                                                <xs:annotation>
                                                                                                    <xs:documentation>Demanda complementar de serviços</xs:documentation>
                                                                                                </xs:annotation>
                                                                                            </xs:enumeration>
                                                                                        </xs:restriction>
                                                                                    </xs:simpleType>
                                                                                </xs:element>
                                                                                <xs:element name="justContr" type="TS_texto_999">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Justificativa da hipótese legal</xs:documentation>
                                                                                        <xs:documentation>Descrição do fato determinado que, no caso concreto, justifica a hipótese legal para a contratação de trabalho temporário. O prazo de contratação do trabalho temporário deve ser compatível com o motivo justificador alegado.</xs:documentation>
                                                                                    </xs:annotation>
                                                                                </xs:element>
                                                                                <xs:element name="ideEstabVinc">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Identificação do estabelecimento ao qual o trabalhador está vinculado</xs:documentation>
                                                                                    </xs:annotation>
                                                                                    <xs:complexType>
                                                                                        <xs:sequence>
                                                                                            <xs:element name="tpInsc" type="TS_tpInsc_1_2" />
                                                                                            <xs:element name="nrInsc" type="TS_nrInsc_11_14">
                                                                                                <xs:annotation>
                                                                                                    <xs:documentation>Número de Inscrição</xs:documentation>
                                                                                                    <xs:documentation>Informar o número de inscrição do contratante de serviços, de acordo com o tipo de inscrição informado em {tpInsc}.</xs:documentation>
                                                                                                    <xs:documentation>Validação: Se {tpInsc} for igual a [1], deve ser um CNPJ válido, com 14 (catorze) algarismos. Se {tpInsc} for igual a [2], deve ser um CPF válido.</xs:documentation>
                                                                                                </xs:annotation>
                                                                                            </xs:element>
                                                                                        </xs:sequence>
                                                                                    </xs:complexType>
                                                                                </xs:element>
                                                                                <xs:element name="ideTrabSubstituido" minOccurs="0" maxOccurs="9">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Identificação do(s) trabalhador(es) substituído(s)</xs:documentation>
                                                                                        <xs:documentation>CHAVE_GRUPO: {cpfTrabSubst}</xs:documentation>
                                                                                        <xs:documentation>CONDICAO_GRUPO: O (se {hipLeg} = [1]); N (nos demais casos)</xs:documentation>
                                                                                    </xs:annotation>
                                                                                    <xs:complexType>
                                                                                        <xs:sequence>
                                                                                            <xs:element name="cpfTrabSubst" type="TS_cpf">
                                                                                                <xs:annotation>
                                                                                                    <xs:documentation>Cpf do trabalhador substituído</xs:documentation>
                                                                                                    <xs:documentation>Validação: Deve ser um CPF válido.</xs:documentation>
                                                                                                </xs:annotation>
                                                                                            </xs:element>
                                                                                        </xs:sequence>
                                                                                    </xs:complexType>
                                                                                </xs:element>
                                                                            </xs:sequence>
                                                                        </xs:complexType>
                                                                    </xs:element>
                                                                    <xs:element name="aprend" minOccurs="0" type="T_aprend">
                                                                        <xs:annotation>
                                                                            <xs:documentation>Informações relacionadas ao aprendiz</xs:documentation>
                                                                            <xs:documentation>CONDICAO_GRUPO: N (se {codCateg} diferente de [103]); OC (se {codCateg} = [103] e se grupo {desligamento} não estiver preenchido); F (nos demais casos)</xs:documentation>
                                                                        </xs:annotation>
                                                                    </xs:element>
                                                                </xs:sequence>
                                                            </xs:complexType>
                                                        </xs:element>
                                                        <xs:element name="infoEstatutario">
                                                            <xs:annotation>
                                                                <xs:documentation>Informações de Trabalhador Estatutário</xs:documentation>
                                                                <xs:documentation>CONDICAO_GRUPO: O (se {tpRegTrab} = [2]); N (nos demais casos)</xs:documentation>
                                                            </xs:annotation>
                                                            <xs:complexType>
                                                                <xs:sequence>
                                                                    <xs:element name="tpProv">
                                                                        <xs:simpleType>
                                                                            <xs:annotation>
                                                                                <xs:documentation>Preencher com o tipo de provimento</xs:documentation>
                                                                                <xs:documentation>Validação: Os valores [3, 4, 5, 6, 7, 8, 9] só são permitidos se a natureza jurídica do declarante for Administração Pública (grupo [1]). Se {codCateg} = [302], deve ser preenchido com [2].</xs:documentation>
                                                                            </xs:annotation>
                                                                            <xs:restriction base="xs:byte">
                                                                                <xs:enumeration value="1">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Nomeação em cargo efetivo</xs:documentation>
                                                                                    </xs:annotation>
                                                                                </xs:enumeration>
                                                                                <xs:enumeration value="2">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Nomeação exclusivamente em cargo em comissão</xs:documentation>
                                                                                    </xs:annotation>
                                                                                </xs:enumeration>
                                                                                <xs:enumeration value="3">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Incorporação (militar)</xs:documentation>
                                                                                    </xs:annotation>
                                                                                </xs:enumeration>
                                                                                <xs:enumeration value="4">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Matrícula (militar)</xs:documentation>
                                                                                    </xs:annotation>
                                                                                </xs:enumeration>
                                                                                <xs:enumeration value="5">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Redistribuição</xs:documentation>
                                                                                    </xs:annotation>
                                                                                </xs:enumeration>
                                                                                <xs:enumeration value="6">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Diplomação</xs:documentation>
                                                                                    </xs:annotation>
                                                                                </xs:enumeration>
                                                                                <xs:enumeration value="7">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Contratação por tempo determinado</xs:documentation>
                                                                                    </xs:annotation>
                                                                                </xs:enumeration>
                                                                                <xs:enumeration value="8">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Remoção (em caso de alteração do órgão declarante)</xs:documentation>
                                                                                    </xs:annotation>
                                                                                </xs:enumeration>
                                                                                <xs:enumeration value="9">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Designação</xs:documentation>
                                                                                    </xs:annotation>
                                                                                </xs:enumeration>
                                                                                <xs:enumeration value="10">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Mudança de CPF</xs:documentation>
                                                                                    </xs:annotation>
                                                                                </xs:enumeration>
                                                                                <xs:enumeration value="99">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Outros não relacionados acima</xs:documentation>
                                                                                    </xs:annotation>
                                                                                </xs:enumeration>
                                                                            </xs:restriction>
                                                                        </xs:simpleType>
                                                                    </xs:element>
                                                                    <xs:element name="dtExercicio" type="xs:date">
                                                                        <xs:annotation>
                                                                            <xs:documentation>Data da entrada em exercício pelo servidor</xs:documentation>
                                                                            <xs:documentation>Validação: Devem ser observadas as seguintes regras:</xs:documentation>
                                                                            <xs:documentation>a) Deve ser posterior à data de nascimento do trabalhador;</xs:documentation>
                                                                            <xs:documentation>b) Se {cadIni} = [S], deve ser anterior à data de início da obrigatoriedade dos eventos não periódicos para o empregador/ente público no eSocial;</xs:documentation>
                                                                            <xs:documentation>c) Se {cadIni} = [N] e {tpProv} diferente de [5, 8, 10], deve ser igual ou posterior à data de início da obrigatoriedade dos eventos não periódicos para o empregador/ente público no eSocial.</xs:documentation>
                                                                        </xs:annotation>
                                                                    </xs:element>
                                                                    <xs:element name="tpPlanRP" minOccurs="0" type="TS_tpPlanRP">
                                                                        <xs:annotation>
                                                                            <xs:documentation>Tipo de plano de segregação da massa</xs:documentation>
                                                                            <xs:documentation>Validação: Preenchimento obrigatório e exclusivo se {tpRegPrev} = [2].</xs:documentation>
                                                                        </xs:annotation>
                                                                    </xs:element>
                                                                    <xs:element name="indTetoRGPS" type="TS_sim_nao" minOccurs="0">
                                                                        <xs:annotation>
                                                                            <xs:documentation>Informar se o servidor está sujeito ao teto do Regime Geral pela instituição do regime de previdência complementar</xs:documentation>
                                                                            <xs:documentation>Validação: Preenchimento obrigatório e exclusivo se {tpRegPrev} = [2].</xs:documentation>
                                                                        </xs:annotation>
                                                                    </xs:element>
                                                                    <xs:element name="indAbonoPerm" type="TS_sim_nao" minOccurs="0">
                                                                        <xs:annotation>
                                                                            <xs:documentation>Indicar se o servidor recebe abono permanência</xs:documentation>
                                                                            <xs:documentation>Validação: Preenchimento obrigatório e exclusivo se {tpRegPrev} = [2].</xs:documentation>
                                                                        </xs:annotation>
                                                                    </xs:element>
                                                                    <xs:element name="dtIniAbono" type="xs:date" minOccurs="0">
                                                                        <xs:annotation>
                                                                            <xs:documentation>Data de início do abono permanência</xs:documentation>
                                                                            <xs:documentation>Validação: Informação obrigatória e exclusiva se {indAbonoPerm} = [S]. Se preenchida, devem ser observadas as seguintes regras:</xs:documentation>
                                                                            <xs:documentation>a) Deve ser igual ou posterior à data de exercício do servidor;</xs:documentation>
                                                                            <xs:documentation>b) Se {cadIni} = [S], deve ser anterior à data de início da obrigatoriedade dos eventos não periódicos para o ente público;</xs:documentation>
                                                                            <xs:documentation>c) Se {cadIni} = [N], deve ser igual ou anterior à data da transferência ou alteração do CPF do servidor ({sucessaoVinc/dtTransf} ou {mudancaCPF/dtAltCPF}).</xs:documentation>
                                                                            <xs:documentation>Não informar se {tpProv} diferente de [5, 8, 10].</xs:documentation>
                                                                        </xs:annotation>
                                                                    </xs:element>
                                                                </xs:sequence>
                                                            </xs:complexType>
                                                        </xs:element>
                                                    </xs:choice>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="infoContrato">
                                            <xs:annotation>
                                                <xs:documentation>Informações do Contrato de Trabalho</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="nmCargo" minOccurs="0" type="TS_texto_100">
                                                        <xs:annotation>
                                                            <xs:documentation>Nome do Cargo</xs:documentation>
                                                            <xs:documentation>Validação: O preenchimento é obrigatório, exceto se for relativo a servidor nomeado em cargo em comissão ({tpRegTrab} = [2] e {tpProv} = [2]).</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="CBOCargo" minOccurs="0" type="TS_cbo">
                                                        <xs:annotation>
                                                            <xs:documentation>Classificação Brasileira de Ocupação</xs:documentation>
                                                            <xs:documentation>Validação: Informação obrigatória e exclusiva se {nmCargo} for preenchido. Se informado, deve ser um código existente na tabela de CBO, com 6 (seis) posições.</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="dtIngrCargo" type="xs:date" minOccurs="0">
                                                        <xs:annotation>
                                                            <xs:documentation>Data de ingresso do servidor no cargo</xs:documentation>
                                                            <xs:documentation>Validação: Não preencher se {tpRegTrab} = [1] ou se {CBOCargo} não for informado. Se preenchida, devem ser observadas as seguintes regras:</xs:documentation>
                                                            <xs:documentation>a) Deve ser igual ou posterior à data de exercício do servidor;</xs:documentation>
                                                            <xs:documentation>b) Se {cadIni} = [S], deve ser anterior à data de início da obrigatoriedade dos eventos não periódicos para o ente público;</xs:documentation>
                                                            <xs:documentation>c) Se {cadIni} = [N], deve ser igual ou anterior à data da transferência ou alteração do CPF do servidor ({sucessaoVinc/dtTransf} ou {mudancaCPF/dtAltCPF}).</xs:documentation>
                                                            <xs:documentation>Não informar se {tpProv} diferente de [5, 8, 10].</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="nmFuncao" minOccurs="0" type="TS_texto_100">
                                                        <xs:annotation>
                                                            <xs:documentation>Nome da Função de confiança/Cargo em Comissão</xs:documentation>
                                                            <xs:documentation>Validação: Preenchimento obrigatório se for relativo a servidor nomeado em cargo em comissão ({tpRegTrab} = [2] e {tpProv} = [2]).</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="CBOFuncao" minOccurs="0" type="TS_cbo">
                                                        <xs:annotation>
                                                            <xs:documentation>Classificação Brasileira de Ocupação</xs:documentation>
                                                            <xs:documentation>Validação: Informação obrigatória e exclusiva se {nmFuncao} for preenchido. Se informado, deve ser um código existente na tabela de CBO, com 6 (seis) posições.</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="acumCargo" type="TS_sim_nao" minOccurs="0">
                                                        <xs:annotation>
                                                            <xs:documentation>Indicar se é um cargo acumulável</xs:documentation>
                                                            <xs:documentation>Validação: Preenchimento obrigatório se {cadIni} = [N] e se a natureza jurídica do declarante for igual a [1XXX, 2011, 2038].</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="codCateg" type="TS_codCateg" />
                                                    <xs:element name="remuneracao" type="T_remuneracao" minOccurs="0">
                                                        <xs:annotation>
                                                            <xs:documentation>Informações da remuneração e periodicidade de pagamento</xs:documentation>
                                                            <xs:documentation>CONDICAO_GRUPO: N (se {tpRegTrab} = [2]); O (se {tpRegTrab} = [1] e se grupo {desligamento} não estiver preenchido); F (nos demais casos)</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="duracao" minOccurs="0">
                                                        <xs:annotation>
                                                            <xs:documentation>Duração do Contrato de Trabalho</xs:documentation>
                                                            <xs:documentation>CONDICAO_GRUPO: N (se {tpRegTrab} = [2]); O (se {tpRegTrab} = [1] e se grupo {desligamento} não estiver preenchido); F (nos demais casos)</xs:documentation>
                                                        </xs:annotation>
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="tpContr" type="TS_tpContr" />
                                                                <xs:element name="dtTerm" type="xs:date" minOccurs="0">
                                                                    <xs:annotation>
                                                                        <xs:documentation>Data do Término</xs:documentation>
                                                                        <xs:documentation>Data do término do contrato por prazo determinado.</xs:documentation>
                                                                        <xs:documentation>Validação: O preenchimento é obrigatório se {tpContr} = [2]. Não informar se {tpContr} = [1]. Se preenchido, deve ser igual ou posterior à data de admissão (no caso de sucessão ou de mudança de CPF, igual ou posterior à data da transferência ou à data de alteração do CPF, respectivamente).</xs:documentation>
                                                                    </xs:annotation>
                                                                </xs:element>
                                                                <xs:element name="clauAssec" type="TS_sim_nao" minOccurs="0">
                                                                    <xs:annotation>
                                                                        <xs:documentation>Indicativo de cláusula assecuratória</xs:documentation>
                                                                        <xs:documentation>Indicar se o contrato por prazo determinado contém cláusula assecuratória do direito recíproco de rescisão antes da data de seu término.</xs:documentation>
                                                                        <xs:documentation>Validação: O preenchimento é obrigatório se {tpContr} = [2, 3]. Não preencher se {tpContr} = [1].</xs:documentation>
                                                                    </xs:annotation>
                                                                </xs:element>
                                                                <xs:element name="objDet" minOccurs="0" type="TS_texto_255">
                                                                    <xs:annotation>
                                                                        <xs:documentation>Indicação do objeto determinante da contratação por prazo determinado (obra, serviço, safra, etc.)</xs:documentation>
                                                                        <xs:documentation>Validação: O preenchimento é obrigatório e exclusivo se {tpContr} = [3].</xs:documentation>
                                                                    </xs:annotation>
                                                                </xs:element>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                    <xs:element name="localTrabalho" minOccurs="0">
                                                        <xs:annotation>
                                                            <xs:documentation>Informações do local de trabalho</xs:documentation>
                                                            <xs:documentation>CONDICAO_GRUPO: O (se grupo {desligamento} não estiver preenchido); F (nos demais casos)</xs:documentation>
                                                        </xs:annotation>
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="localTrabGeral" type="T_localTrabalho" minOccurs="0">
                                                                    <xs:annotation>
                                                                        <xs:documentation>Estabelecimento onde o trabalhador exercerá suas atividades</xs:documentation>
                                                                        <xs:documentation>CONDICAO_GRUPO: N (se {codCateg} = [104, 106]); O (se {codCateg} diferente de [104, 106] e se grupo {desligamento} não estiver preenchido); F (nos demais casos)</xs:documentation>
                                                                    </xs:annotation>
                                                                </xs:element>
                                                                <xs:element name="localTrabDom" type="T_endereco_Brasil" minOccurs="0">
                                                                    <xs:annotation>
                                                                        <xs:documentation>Endereço de trabalho do trabalhador doméstico e trabalhador temporário</xs:documentation>
                                                                        <xs:documentation>CONDICAO_GRUPO: N (se {codCateg} diferente de [104, 106]); O (se {codCateg} = [104, 106] e se grupo {desligamento} não estiver preenchido); F (nos demais casos)</xs:documentation>
                                                                    </xs:annotation>
                                                                </xs:element>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                    <xs:element name="horContratual" minOccurs="0" type="T_horContratual">
                                                        <xs:annotation>
                                                            <xs:documentation>Informações do Horário Contratual do Trabalhador</xs:documentation>
                                                            <xs:documentation>CONDICAO_GRUPO: O (se {tpRegJor} = [1] e se grupo {desligamento} não estiver preenchido); OC (se {tpRegJor} diferente de [1] e se grupo {desligamento} não estiver preenchido); F (nos demais casos)</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="alvaraJudicial" minOccurs="0" type="T_alvaraJudicial">
                                                        <xs:annotation>
                                                            <xs:documentation>Dados do Alvará Judicial</xs:documentation>
                                                            <xs:documentation>CONDICAO_GRUPO: OC (se grupo {desligamento} não estiver preenchido); F (nos demais casos)</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="observacoes" minOccurs="0" maxOccurs="99">
                                                        <xs:annotation>
                                                            <xs:documentation>Observações do contrato de trabalho</xs:documentation>
                                                            <xs:documentation>CONDICAO_GRUPO: OC (se grupo {desligamento} não estiver preenchido); F (nos demais casos)</xs:documentation>
                                                        </xs:annotation>
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="observacao" type="TS_texto_255">
                                                                    <xs:annotation>
                                                                        <xs:documentation>Observação</xs:documentation>
                                                                    </xs:annotation>
                                                                </xs:element>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                    <xs:element name="treiCap" minOccurs="0" maxOccurs="99" type="T_treiCap">
                                                        <xs:annotation>
                                                            <xs:documentation>Treinamentos, Capacitações, Exercícios Simulados e Outras Anotações</xs:documentation>
                                                            <xs:documentation>CHAVE_GRUPO: {codTreiCap}</xs:documentation>
                                                            <xs:documentation>CONDICAO_GRUPO: OC (se grupo {desligamento} não estiver preenchido); F (nos demais casos)</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="sucessaoVinc" minOccurs="0">
                                            <xs:annotation>
                                                <xs:documentation>Grupo de informações da sucessão de vínculo trabalhista</xs:documentation>
                                                <xs:documentation>CONDICAO_GRUPO: O (se {tpAdmissao} = [2, 3, 4] ou {tpProv} = [5, 8]); N (nos demais casos)</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="tpInsc">
                                                        <xs:simpleType>
                                                            <xs:annotation>
                                                                <xs:documentation>Tipo de inscrição</xs:documentation>
                                                                <xs:documentation>Preencher com o código correspondente ao tipo de inscrição, conforme Tabela 05.</xs:documentation>
                                                                <xs:documentation>Validação: Deve ser igual a [1] (CNPJ), [2] (CPF), [5] (CGC) ou [6] (CEI).</xs:documentation>
                                                                <xs:documentation>Somente é possível informar [5] se {dtTrans} for igual ou anterior a 01/07/1988.</xs:documentation>
                                                                <xs:documentation>Somente é possível informar [6] se {dtTransf} for igual ou anterior a 31/12/2011.</xs:documentation>
                                                            </xs:annotation>
                                                            <xs:restriction base="xs:byte">
                                                                <xs:enumeration value="1">
                                                                    <xs:annotation>
                                                                        <xs:documentation>CNPJ</xs:documentation>
                                                                    </xs:annotation>
                                                                </xs:enumeration>
                                                                <xs:enumeration value="2">
                                                                    <xs:annotation>
                                                                        <xs:documentation>CPF</xs:documentation>
                                                                    </xs:annotation>
                                                                </xs:enumeration>
                                                                <xs:enumeration value="5">
                                                                    <xs:annotation>
                                                                        <xs:documentation>CGC</xs:documentation>
                                                                    </xs:annotation>
                                                                </xs:enumeration>
                                                                <xs:enumeration value="6">
                                                                    <xs:annotation>
                                                                        <xs:documentation>CEI</xs:documentation>
                                                                    </xs:annotation>
                                                                </xs:enumeration>
                                                            </xs:restriction>
                                                        </xs:simpleType>
                                                    </xs:element>
                                                    <xs:element name="nrInsc">
                                                        <xs:simpleType>
                                                            <xs:annotation>
                                                                <xs:documentation>Informar o número de inscrição do empregador anterior, de acordo com o tipo de inscrição indicado no campo {sucessaoVinc}/{tpInsc}.</xs:documentation>
                                                                <xs:documentation>Validação: Deve ser um número de inscrição válido diferente da inscrição do declarante, considerando as particularidades aplicadas à informação de CNPJ de órgão público em S-1000.</xs:documentation>
                                                                <xs:documentation>Se {tpInsc} = [1], deve possuir 14 (catorze) algarismos e ser diferente do CNPJ base do empregador e dos estabelecimentos informados através do evento S-1005.</xs:documentation>
                                                                <xs:documentation>Se {tpInsc} = [2], deve possuir 11 (onze) algarismos.</xs:documentation>
                                                                <xs:documentation>Se {tpInsc} = [5], deve possuir somente algarismos.</xs:documentation>
                                                                <xs:documentation>Se {tpInsc} = [6], deve possuir 12 (doze) algarismos.</xs:documentation>
                                                            </xs:annotation>
                                                            <xs:restriction base="xs:string">
                                                                <xs:pattern value="\d{8,14}" />
                                                            </xs:restriction>
                                                        </xs:simpleType>
                                                    </xs:element>
                                                    <xs:element name="matricAnt" minOccurs="0" type="TS_codigo_esocial">
                                                        <xs:annotation>
                                                            <xs:documentation>Matrícula no empregador anterior</xs:documentation>
                                                            <xs:documentation>Validação: O preenchimento é obrigatório se {cadIni} = [N].</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="dtTransf" type="xs:date">
                                                        <xs:annotation>
                                                            <xs:documentation>Data da transferência</xs:documentation>
                                                            <xs:documentation>Preencher com a data da transferência do empregado para o empregador declarante.</xs:documentation>
                                                            <xs:documentation>Validação: Devem ser observadas as seguintes regras:</xs:documentation>
                                                            <xs:documentation>a) Deve ser posterior à data de admissão do trabalhador;</xs:documentation>
                                                            <xs:documentation>b) Se {cadIni} = [S], deve ser anterior à data de início da obrigatoriedade dos eventos não periódicos para o empregador;</xs:documentation>
                                                            <xs:documentation>c) Se {cadIni} = [N], deve ser igual ou posterior à data de início da obrigatoriedade dos eventos não periódicos para o empregador.</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="observacao" minOccurs="0" type="TS_texto_255">
                                                        <xs:annotation>
                                                            <xs:documentation>Observação</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="transfDom" minOccurs="0">
                                            <xs:annotation>
                                                <xs:documentation>Informações do empregado doméstico transferido de outro representante da mesma unidade familiar</xs:documentation>
                                                <xs:documentation>CONDICAO_GRUPO: O (se {tpAdmissao} for igual [5]); N (nos demais casos)</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="cpfSubstituido" type="TS_cpf">
                                                        <xs:annotation>
                                                            <xs:documentation>CPF do representante anterior</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="matricAnt" minOccurs="0" type="TS_codigo_esocial">
                                                        <xs:annotation>
                                                            <xs:documentation>Matrícula no empregador anterior</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="dtTransf" type="xs:date">
                                                        <xs:annotation>
                                                            <xs:documentation>Data da transferência</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="mudancaCPF" minOccurs="0">
                                            <xs:annotation>
                                                <xs:documentation>Informações de mudança de CPF do trabalhador</xs:documentation>
                                                <xs:documentation>CONDICAO_GRUPO: O (se {tpAdmissao} = [6] ou {tpProv} = [10]); N (nos demais casos)</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="cpfAnt" type="TS_cpf">
                                                        <xs:annotation>
                                                            <xs:documentation>CPF anterior</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="matricAnt" type="TS_codigo_esocial">
                                                        <xs:annotation>
                                                            <xs:documentation>Matrícula no empregador anterior</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="dtAltCPF" type="xs:date">
                                                        <xs:annotation>
                                                            <xs:documentation>Data de alteração do CPF</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="observacao" minOccurs="0" type="TS_texto_255">
                                                        <xs:annotation>
                                                            <xs:documentation>Observação</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="afastamento" minOccurs="0">
                                            <xs:annotation>
                                                <xs:documentation>Informações de afastamento do trabalhador</xs:documentation>
                                                <xs:documentation>CONDICAO_GRUPO: N (se grupo {desligamento} estiver preenchido); OC (nos demais casos)</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="dtIniAfast" type="xs:date">
                                                        <xs:annotation>
                                                            <xs:documentation>Data de início do afastamento</xs:documentation>
                                                            <xs:documentation>Validação: Devem ser observadas as seguintes regras:</xs:documentation>
                                                            <xs:documentation>a) Deve ser igual ou posterior à data de admissão/exercício do trabalhador;</xs:documentation>
                                                            <xs:documentation>b) Se {cadIni} = [S], deve ser anterior à data de início da obrigatoriedade dos eventos não periódicos para o empregador;</xs:documentation>
                                                            <xs:documentation>c) Se {cadIni} = [N], deve ser anterior à data da transferência ou alteração do CPF do empregado ({sucessaoVinc/dtTransf}, {transfDom/dtTransf} ou {mudancaCPF/dtAltCPF}).</xs:documentation>
                                                            <xs:documentation>Não informar se {tpAdmissao} = [1] ou {tpProv} diferente de [5, 8, 10].</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="codMotAfast" type="TS_codMotAfast" />
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="desligamento" minOccurs="0">
                                            <xs:annotation>
                                                <xs:documentation>Informações de desligamento do trabalhador</xs:documentation>
                                                <xs:documentation>CONDICAO_GRUPO: N (se (grupo {afastamento} ou {cessao} estiver preenchido) ou (se {tpAdmissao} = [6] ou {tpProv} = [10])); OC (nos demais casos)</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="dtDeslig" type="xs:date">
                                                        <xs:annotation>
                                                            <xs:documentation>Data do Desligamento</xs:documentation>
                                                            <xs:documentation>Preencher com a data do último dia trabalhado para o respectivo vínculo.</xs:documentation>
                                                            <xs:documentation>Validação: Devem ser observadas as seguintes regras:</xs:documentation>
                                                            <xs:documentation>a) Deve ser igual ou posterior à data de admissão/exercício do trabalhador;</xs:documentation>
                                                            <xs:documentation>b) Se {cadIni} = [S], deve ser anterior à data de início da obrigatoriedade dos eventos não periódicos para o empregador;</xs:documentation>
                                                            <xs:documentation>c) Se {cadIni} = [N], deve ser anterior à data da transferência do empregado ({sucessaoVinc/dtTransf} ou {transfDom/dtTransf}).</xs:documentation>
                                                            <xs:documentation>Não informar se {tpAdmissao} = [1] ou {tpProv} diferente de [5, 8].</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="cessao" minOccurs="0">
                                            <xs:annotation>
                                                <xs:documentation>Informações de cessão/exercício em outro órgão do trabalhador</xs:documentation>
                                                <xs:documentation>CONDICAO_GRUPO: N (se grupo {desligamento} estiver preenchido); OC (nos demais casos)</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="dtIniCessao" type="xs:date">
                                                        <xs:annotation>
                                                            <xs:documentation>Data de início da cessão/exercício em outro órgão.</xs:documentation>
                                                            <xs:documentation>Validação: Devem ser observadas as seguintes regras:</xs:documentation>
                                                            <xs:documentation>a) Deve ser igual ou posterior à data de admissão/exercício do trabalhador;</xs:documentation>
                                                            <xs:documentation>b) Se {cadIni} = [S], deve ser anterior à data de início da obrigatoriedade dos eventos não periódicos para o empregador/ente público;</xs:documentation>
                                                            <xs:documentation>c) Se {cadIni} = [N], deve ser anterior à data da transferência ou alteração do CPF do empregado ({sucessaoVinc/dtTransf} ou {mudancaCPF/dtAltCPF}).</xs:documentation>
                                                            <xs:documentation>Não informar se {tpAdmissao} = [1] ou {tpProv} diferente de [5, 8, 10].</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                        <xs:attribute name="Id" use="required" type="TS_Id" />
                    </xs:complexType>
                </xs:element>
                <xs:element ref="ds:Signature" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>
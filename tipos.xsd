                </xs:annotation>
            </xs:element>
            <xs:element name="ideTabRubr" type="TS_ideTabRubr">
                <xs:annotation>
                    <xs:documentation>Preencher com o identificador da Tabela de Rubricas para a rubrica definida em {codRubr}(1010_infoRubrica_inclusao_ideRubrica_codRubr).</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="qtdRubr" minOccurs="0" type="TS_qtdRubr" />
            <xs:element name="fatorRubr" minOccurs="0" type="TS_fator" />
            <xs:element name="vrRubr" type="TS_vrRubr" />
            <xs:element name="indApurIR" type="TS_indApurIR">
                <xs:annotation>
                    <xs:documentation>Indicativo de tipo de apuração de IR.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="T_itensRemun">
        <xs:annotation>
            <xs:documentation>Itens da remuneração do trabalhador</xs:documentation>

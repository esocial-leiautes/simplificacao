            <xs:minLength value="1" />
            <xs:maxLength value="100" />
            <xs:pattern value=".*[^\s].*" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="TS_texto_255">
        <xs:restriction base="xs:string">

            <xs:documentation>Tipo de dependente.</xs:documentation>
            <xs:documentation>Validação: Deve ser um código existente na Tabela 07.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="\d{2}" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="TS_tpRegPrev">
        <xs:restriction base="xs:byte">
            <xs:enumeration value="1">

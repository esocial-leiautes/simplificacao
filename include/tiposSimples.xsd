    <xs:simpleType name="TS_texto_999">
        <xs:restriction base="xs:string">
            <xs:minLength value="1" />
            <xs:maxLength value="999" />
            <xs:pattern value=".*[^\s].*" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="TS_nrProcJud">
        <xs:restriction base="xs:string">
            <xs:length value="20" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="TS_codSusp">
        <xs:annotation>
            <xs:documentation>Código do Indicativo da Suspensão, atribuído pelo empregador em S-1070.</xs:documentation>
            <xs:documentation>Validação: A informação prestada deve estar de acordo com o que foi informado em S-1070.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:pattern value="\d{1,14}" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="TS_codigo_esocial">
        <xs:restriction base="xs:string">

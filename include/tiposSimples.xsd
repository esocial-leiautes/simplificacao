            </xs:enumeration>
            <xs:enumeration value="2">
                <xs:annotation>
                    <xs:documentation>Regime Próprio de Previdência Social - RPPS ou regime dos militares</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="3">
                <xs:annotation>
                    <xs:documentation>Regime de Previdência Social no Exterior</xs:documentation>
                </xs:annotation>

                                                    <xs:element name="tpInsc" type="TS_tpInsc_1_2_3_4" />
                                                    <xs:element name="nrInsc" type="TS_nrInsc_11_12_14">
                                                        <xs:annotation>
                                                            <xs:documentation>Informar o número de inscrição do contribuinte de acordo com o tipo de inscrição indicado no campo {tpInsc}.</xs:documentation>
                                                            <xs:documentation>A inscrição informada segue o evento que deu origem ao presente evento (S-1270 ou S-5003).</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="ideLotacao" minOccurs="1" maxOccurs="unbounded">
